{
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "extends": [
        "google",
        "plugin:react/recommended"
    ],
    "plugins": [
        "babel"
    ],
    "rules": {
        "require-jsdoc": "off",
        "quotes": ["error", "single", { "avoidEscape": true }],
        "babel/semi": true
    }
}